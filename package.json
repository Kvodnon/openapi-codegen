  "devDependencies": {
    "commander": "8.3.0",
    "inquirer": "8.2.1",
    "js-yaml": "4.1.0",
    "recursive-readdir-sync": "1.0.6",
    "traverse": "0.6.6"
  },
  "sideEffects": false
}
